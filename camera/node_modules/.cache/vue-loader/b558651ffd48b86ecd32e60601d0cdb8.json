        <p v-if=\"!this.oPictures.length\">사진이 없습니다. 추가해 주세요!</p>\r\n      </v-col>\r\n      <!-- 하단우측에 포스트 추가 버튼 표시 -->\r\n      <v-btn @click=\"$router.push('/post')\" color=\"red\" dark fixed bottom right fab>\r\n        <v-icon>add</v-icon>\r\n      </v-btn>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n<script>\r\n  // 파이어베이스 DB 객체 가져옴\r\n  import {\r\n    oPicturesinDB\r\n  } from '@/datasources/firebase'\r\n  export default {\r\n    name: 'App',\r\n    data() {\r\n      return {\r\n        oPictures: []\r\n      } // 사진 데이터 저장 변수\r\n    },\r\n    // 파이어베이스와 연결된 뷰파이어 oPictures 객체 준비\r\n    firebase: {\r\n      oPictures: oPicturesinDB\r\n    },\r\n    methods: {\r\n      // 라우터를 이용해서 세부페이지로 이동할 때 사진의 ID 전달\r\n      fnDisplayInfo(pID) {\r\n        this.$router.push({\r\n          name: 'info_page',\r\n          params: {\r\n            p_id: pID\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style>\r\n  /* 마우스 커서가 손모양이 되도록 설정 */\r\n  .pointer {\r\n    cursor: pointer;\r\n  }\r\n</style>"]}]}